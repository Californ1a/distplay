{
  "name": "distplay",
  "version": "0.1.0",
  "description": "input display",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:win32": "electron-packager ./ --platform=win32 --arch=ia32 --overwrite",
    "pack:win64": "electron-packager ./ --platform=win32 --arch=x64 --overwrite",
    "pack:linux": "electron-packager ./ --platform=linux --arch=x64 --overwrite",
    "build": "npm run pack:win32 && npm run pack:win64 && npm run pack:linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TntMatthew/distplay.git"
  },
  "author": "TntMatthew",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TntMatthew/distplay/issues"
  },
  "homepage": "https://github.com/TntMatthew/distplay#readme",
  "devDependencies": {
    "electron": "1.8.2"
  },
  "dependencies": {
    "electron-require": "^0.3.0",
    "ini": "^1.3.5"
  }
}
