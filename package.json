{
	"name": "distplay",
	"version": "1.4.1",
	"description": "input display",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"pack:win": "electron-zip-packager ./ --platform=win32 --arch=x64 --overwrite --ignore=build --out=dist --icon=build/icon.ico",
		"pack:mac": "electron-zip-packager ./ --platform=darwin --arch=x64 --overwrite --ignore=build --out=dist --icon=build/icon.ico",
		"pack:linux": "electron-zip-packager ./ --platform=linux --arch=x64 --overwrite --ignore=build --out=dist --icon=build/icon.png",
		"build": "npm run pack:win && npm run pack:mac && npm run pack:linux"
	},
	"build": {
		"appId": "com.distplay.app",
		"productName": "Distplay"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Californ1a/distplay.git"
	},
	"author": "TntMatthew & Californ1a",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Californ1a/distplay/issues"
	},
	"homepage": "https://github.com/Californ1a/distplay#readme",
	"devDependencies": {
		"electron": "^3.0.10",
		"electron-zip-packager": "^4.0.2"
	},
	"dependencies": {
		"distplay-settings-panel-theme": "github:Californ1a/distplay-settings-panel-theme",
		"electron-context-menu": "^0.15.0",
		"electron-is-dev": "^0.3.0",
		"electron-require": "^0.3.0",
		"electron-store": "^1.3.0",
		"settings-panel": "^1.8.17",
		"update-electron-app": "^1.5.0"
	}
}
